   "source": [
    "def hanoi_tower(n, fr, tmp, to):\n",
    "    if (n==1):\n",
    "        print(\"원판 1: %s --> %s\" % (fr, to))\n",
    "    else :\n",
    "        hanoi_tower(n-1, fr, to, tmp)\n",
    "        print(\"원판 %d: %s --> %s\" % (n,fr, to))\n",
    "        hanoi_tower(n-1, tmp, fr, to)\n",
    "\n",
    "hanoi_tower(4, 'A', 'B', 'C')"
   ]
